<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/property_management/static/src/js/property.js"></script>
        </xpath>
    </template>

    <template id="top_search" name="top search">
        <div class="cont-nav">
            <nav class="navbar navbar-expand-lg navbar-light fluid navbar-searcher">
                <div class="searcher-cont" id="menu">
            

                    <form class="row" style="justify-content: center">
                        <div class="accordion" id="accordion" >
                            <div class="row searcher-nav">
                                <div class="cont-uni-searcher">
                                    <span class="span-select">OPERACIÓN</span>
                                    <select aria-label="OPERACION" class="form-select span-select-li" id="real_state_op" name="real_state_op">
                                        <t t-as="operation" t-foreach="operations">
                                            <option class="span-select-li" t-att-selected="operation[0] == get_vals['real_state_op'] if 'real_state_op' in get_vals else False" t-att-value="operation[0]" t-esc="operation[1]"/>
                                        </t>
                                    </select>
                                </div>
                                <div class="cont-uni-searcher">
                                    <span class="span-select">PROPIEDAD</span>
                                    <select aria-label="TIPO DE PROPIEDAD" class="form-select span-select-li" id="property_type_id" name="property_type_id">
                                        <option value="" class="span-select-li">Todas</option>
                                        <t t-as="property_type" t-foreach="property_type_ids">
                                            <option class="span-select-li" t-att-selected="str(property_type.id) ==  get_vals['property_type_id'] if 'property_type_id' in get_vals else False" t-att-value="property_type.id" t-field="property_type.name"/>
                                        </t>
                                    </select>
                                </div>
                                <div class="cont-uni-searcher keyword">
                                    <label class="form-label-searcher">COMUNA O PALABRA CLAVE</label>
                                    <div class="btn-group btn-cont-searcher " data-toggle="button">
                                        <input class="form-control-pp span-select-li" id="address" name="address" placeholder="  La Reina, RM, Chile" t-att-value="get_vals['address'] if  'address' in get_vals else ''"/>
                                    </div>
                                </div>
                            </div>
                            <div class="accordion-content " id="property_accordion_navbar">
                                <div class="cont-uni-searcher price-min">
                                    <span class="span-select">PRECIO MÍN.</span>
                                    <input class="form-select-price span-select-li" id="amount_min" name="amount_min" placeholder="  -" t-att-value="get_vals['amount_min'] if  'amount_min' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher price-max">
                                    <span class="span-select">PRECIO MÁX.</span>
                                    <input class="form-select-price span-select-li" id="amount_max" name="amount_max" placeholder="  -" t-att-value="get_vals['amount_max'] if  'amount_max' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher-currency currency">
                                    <span class="span-select">MONEDA</span>
                                    <select aria-label="MONEDA" class="form-select-price-currency span-select-li" id="currency" name="currency">
                                        <t t-as="currency_id" t-foreach="currency_ids">
                                            <option class="span-select-li" t-att-selected="True and currency and currency_id.id == currency.id or False" t-att-value="currency_id.name" t-field="currency_id.symbol"/>
                                            <!-- <option class="span-select-li" t-att-selected="str(currency_id.name) ==  get_vals['currency'] if 'currency' in get_vals else False" t-att-value="currency_id.name" t-field="currency_id.symbol"/> -->
                                        </t>
                                    </select>
                                </div>
                                <div class="cont-uni-searcher room-min">
                                    <span class="span-select">DORMITORIO MÍN.</span>
                                    <input class="form-select-room span-select-li" id="bedroom_quantity_min" name="bedroom_quantity_min" placeholder="  -" t-att-value="get_vals['bedroom_quantity_min'] if  'bedroom_quantity_min' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher room-max">
                                    <span class="span-select">DORMITORIO MÁX.</span>
                                    <input class="form-select-room span-select-li" id="bedroom_quantity_max" name="bedroom_quantity_max" placeholder="  -" t-att-value="get_vals['bedroom_quantity_max'] if  'bedroom_quantity_max' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher bath-min">
                                    <span class="span-select">BAÑOS MÍN.</span>
                                    <input class="form-select-price span-select-li" id="dependecies_min" name="dependecies_min" placeholder="  -" t-att-value="get_vals['dependecies_min'] if  'dependecies_min' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher bath-max">
                                    <span class="span-select">BAÑOS MÁX.</span>
                                    <input class="form-select-price span-select-li" id="dependecies_max" name="dependecies_max" placeholder="  -" t-att-value="get_vals['dependecies_max'] if  'dependecies_max' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher sup-min">
                                    <span class="span-select">SUPERFICIE MÍN.</span>
                                    <input class="form-select-price span-select-li" id="total_area_min" name="total_area_min" placeholder="  -" t-att-value="get_vals['total_area_min'] if  'total_area_min' in get_vals else ''"/>
                                </div>
                                <div class="cont-uni-searcher sup-max">
                                    <span class="span-select">SUPERFICIE MÁX.</span>
                                    <input class="form-select-price span-select-li" id="total_area_max" name="total_area_max" placeholder="  -" t-att-value="get_vals['total_area_max'] if  'total_area_max' in get_vals else ''"/>
                                </div>
                            </div>
                            <a class="accordion-toggle">
                                <span class="span-search-av" id="property_search_down"> BUSQUEDA AVANZADA </span>
                                <span class="span-search-av" id="property_search_up"> BUSQUEDA AVANZADA </span>
                                <i class="fa fa-caret-up property-search-button-up" id="property_search_button_up"></i>
                                <i class="fa fa-caret-down property-search-button-down" id="property_search_button_down"></i>
                            </a>
                        </div>
                            <div class="cont-uni-searcher">      
                                <button class="btn-searcher btn btn-outline-primary" type="submit">
                                <svg class="bi bi-search searcher-elements-btn" fill="currentColor" height="16" viewBox="0 0 16 16" width="16"
                                    xmlns="http://www.w3.org/2000/svg">
                                    <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"/>
                                </svg>
                                <span class="searcher-elements-btn">APLICAR</span>
                                </button>
                            </div>
                    </form>
                </div>
            </nav>
        </div>
        <script type="text/javascript" src="/property_management/static/src/js/searcher_menu.js"></script>
    </template>

    <template id="property_item" name="property item">
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12 col-xs-12">
        <div class="container box-border-property">
            <div class="container-fluid">
                <a t-att-href="property.website_url">
                    <t t-call="property_management.property_item_image"/>
                </a>
            </div>
            <div class="container property-information">
                <!-- <div class="property-title"><a t-att-href="property.website_url + '?'  + keep_query('*')" t-field="property.name"/> </div> -->
                <!-- <a t-attf-href="/property/#{property.id}"> -->
                <div class="property-title" t-esc="property.head">
                    <a t-att-href="property.website_url"/>
                </div>
                <div class="property-description">
                    <li class="li-property-description">
                        <span class="span-description" t-esc="property.city"/>,
                        <span class="span-description" t-esc="property.state_id.name"/>
                    </li>
                </div>
                <div class="property-prices-card">
                    <t t-if="property.real_estate_lease">
                        <li class="property-price">
                            <span class="property-p-label">Arriendo</span>
                            <t class="property-price-currency" t-esc="property.lease_price_currency.symbol"/>&#032;
                            <t class="property-price-valor" t-esc="property.format_int_price(property.lease_price)"/>
                            <!-- <span class="property-price-valor" t-esc="property.lease_price" t-options="{'widget': 'monetary', 'display_currency': property.lease_price_currency}"/> -->
                        </li>
                    </t>
                    <t t-if="property.real_estate_sale">
                        <li class="property-price">
                            <span class="property-p-label">Venta</span>
                            <t class="property-price-currency" t-esc="property.sale_price_currency.symbol"/>&#032;
                            <t class="property-price-valor" t-esc="property.format_int_price(property.sale_price)"/>
                            <!-- <span class="property-price-valor" t-esc="property.sale_price" t-options="{'widget': 'monetary', 'display_currency': property.sale_price_currency}"/> -->
                        </li>
                    </t>
                </div>
                <div class="container-fluid list-icons-general">
                    <ol class="list-attr-property">
                        <li class="li-attr-property bed-li">
                            <img alt="Habitaciones" src="/property_management/static/src/img/bed.svg" class="icon-property-pp"/>
                            <span class="p-icon-txt" t-field="property.bedroom_quantity"/>
                        </li>
                        <li class="li-attr-property bat-li">
                            <img alt="Baños" src="/property_management/static/src/img/bath.svg" class="icon-property-pp"/>
                            <span class="p-icon-txt" t-field="property.dependencies"/>
                        </li>
                        <li class="li-attr-property">
                            <img alt="Medidas C" src="/property_management/static/src/img/ruler.svg" class="icon-property-pp"/>
                            <span class="p-icon-txt" t-field="property.builded_surface"/>
                            <span class="p-icon-txt">m2</span>
                        </li>
                        <li class="li-attr-property">
                            <img alt="Medidas T" src="/property_management/static/src/img/beach-umbrella-.svg" class="icon-property-pp"/>
                            <span class="p-icon-txt" t-field="property.total_area"/>
                            <span class="p-icon-txt">m2</span>
                        </li>
                    </ol>
                </div>
            </div>
        </div>
        </div>
    </template>
    <template id="ordering" name="property_sort">
        <button aria-expanded="false" aria-haspopup="true" class="fa fa-bars btn sort-p" data-toggle="dropdown" type="button">
            <span class="span-sort-property">Ordenar Por</span>
        </button>
        <div class="dropdown-menu">
            <t t-as="sort" t-foreach="searchbar_sortings">
                <a class="dropdown-item" t-att-href="request.httprequest.path + '?' + keep_query('*', sort=sort)" t-esc="searchbar_sortings[sort]['label']"/>
            </t>
        </div>
    </template>
    <template id="properties" name="Propiedades">
        <t t-call="website.layout">
            <div class="wrap">
                <div class="container-fluid"/>
                <t t-call="property_management.top_search"/>
                <div class="container container-property">
                    <div class="row justify-content-between">
                        <div class="col-10">
                            <span class="search-property">Encuentra tu propiedad</span>
                        </div>
                        <div class="col-2" style="align-self: center">
                            <t t-call="property_management.ordering"/>
                        </div>
                    </div>
                    <div class="row" t-if="properties">
                        <t t-as="property" t-foreach="properties">
                            <t t-call="property_management.property_item"/>
                        </t>
                    </div>
                    <div class="products-pager form-inline justify-content-center mt-3">
                        <t t-call="website.pager">
                            <t t-set="_classes">mt-2 ml-md-2</t>
                        </t>
                    </div>
                </div>
            </div>
        </t>
    </template>


