<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="real_state_snippets" inherit_id="website.snippets">
        <xpath expr="//t[@id='default_snippets']" position="after">
            <div class="o_panel" id="snippet_real_state">
                <div class="o_panel_header">real_state</div>
                <div class="o_panel_body">
                    <t t-snippet="property_management.s_real_state_search" t-thumbnail="/website/static/src/img/snippets_thumbs/s_banner.svg">
                        <keywords>Buscador de propiedades</keywords>
                    </t>
                </div>
            </div>
        </xpath>
    </template>
</odoo>